
@model AddManagerViewModel

@{
    ViewBag.Title = "Изтрий мениджър";
}


<h2 class="text-center">@ViewBag.Title</h2>
<hr />

<div class="row">
    <div class="col-sm-12 offset-lg-2 col-lg-8 offset-xl-3 col-xl-6">
        <form method="post">
          
           

            <div class="mb-3">
                <label asp-for="@Model.IdEgn" class="form-label">ЕГН</label>
                <input asp-for="@Model.IdEgn" class="form-control" aria-required="true" />
                <span asp-validation-for="IdEgn" class="text-danger"></span>
            </div>
           
            
    
           
            <div class="mb-3">
                <input class="btn btn-primary" type="submit" value="Изтрий мениджър" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}